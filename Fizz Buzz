class Solution 
{
    public List<String> fizzBuzz(int n) 
    {
        List<String> a= new ArrayList<>(n);

        for(int i=1;i<=n;i++)
        {
            boolean b=i%3==0;
            boolean c=i%5==0;
            if(b&&c)
            a.add("FizzBuzz");
            else if(b)
            a.add("Fizz");
            else if(c)
            a.add("Buzz");
            else
            a.add(String.valueOf(i));
        }
        return a;
    }
}
